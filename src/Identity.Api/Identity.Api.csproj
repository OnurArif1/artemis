    <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="9.0.4" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Artemis.API\Artemis.API.csproj" />
  </ItemGroup>

</Project>
